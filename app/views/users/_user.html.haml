%tr
  %td
    = user.id
  %td
    = user.phone
  %td
    = link_to user.first_name, user_path(user)
  %td
    = link_to(user_path(user), html_options = {method: :delete}) do
      - unless user.admin?
        - if user.open?
          %i.material-icons.md-18
            lock_open
        - else
          %i.material-icons.md-18
            lock
